{
  "name": "webpacktemplate",
  "version": "1.0.0",
  "description": "My template",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config ./build/webpack.dev.conf.js",
    "build": "webpack --config ./build/webpack.build.conf.js",
    "test": "node ./scripts/test.js --env=jsdom"
  },
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "author": "https://github.com/vedees",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vedees/webpack-template.git"
  },
  "bugs": {
    "url": "https://github.com/vedees/webpack-template/issues"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "merge",
    "bable",
    "bable7",
    "scss"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "formik": "^2.1.4",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "^9.7.3",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.3.0",
    "babel-preset-react-app": "^9.1.2",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.0",
    "object-assign": "^4.1.1",
    "postcss-loader": "^7.0.2",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.3.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/build/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.js",
      "<rootDir>/src/**/__tests__/**/*.js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(scss|css)$": "<rootDir>/build/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|scss|css|json)$)": "<rootDir>/build/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
